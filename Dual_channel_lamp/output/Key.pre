for(k=0;k<120;k++){ soft_delay_ms(1); WDT_clear();};
CCP1_set_duty_cycle(0); CCP2_set_duty_cycle(0);
for(k=0;k<120;k++){ soft_delay_ms(1); WDT_clear();};
}
}


void LEDCon_Chg(void)
{
switch(LED_Chg_Cnt)
{
case 0: LEDR1_Vol=250; LEDR2_Vol=0; break;
case 1: LEDR1_Vol=0; LEDR2_Vol=250; break;
case 2: LEDR1_Vol=250; LEDR2_Vol=250; break;
case 3: LEDR1_Vol=2; LEDR2_Vol=2; break;

case 4: LEDR1_Vol=2; LEDR2_Vol=0; break;
case 5: LEDR1_Vol=0; LEDR2_Vol=2; break;

}
PWM_LED1(LEDR1_Vol); PWM_LED2(LEDR2_Vol);
SetRunEn(0,1,LEDR1_Vol);
SetRunEn(1,1,LEDR2_Vol);
}

