void Key_Handle(unsigned char *buf)
{
unsigned char i=0;
unsigned short int k=0;
unsigned char n=0;

if(buf[0]!=0x8C) return;

Time1s_cot = 1000;

KeyVol=((unsigned short int)buf[1]<<8);
KeyVol+=buf[2];

if(KeyVolSV!=KeyVol)
{
KeyVolSV=KeyVol;

if(SysRunTime_3S==0)
{


if((KeyVol==0x0001)||(KeyVol==0x4000))
{

EP_SetRFAdd(&buf[3]);
soft_delay_ms(10);
EP_GetRFAdd(RF_ADDRESS);

for(i=0;i<5;i++)
{
if(RF_ADDRESS[i]!=buf[i+3]) break;
}
if(i==5)
{
soft_delay_ms(5);
RF_ADDSSet(RF_ADDRESS);
soft_delay_ms(5);
EP_SetADDCfg(1);

Pair_LED();

LED_ALLOFF();
LEDR1_Vol=0; LEDR2_Vol=250;
PWM_LED1(LEDR1_Vol);
PWM_LED2(LEDR2_Vol);
SysRunTime_3S=1;


}

return;
}



if((KeyVol==0x0010)||(KeyVol==0x0040))
{
EP_SetADDCfg(0);
soft_delay_ms(5);
RF_ADDSSet(TX_ADDRESS_DEF);
soft_delay_ms(5);

LED_ALLOFF();
LEDR1_Vol=0; LEDR2_Vol=250;
PWM_LED1(LEDR1_Vol);
PWM_LED2(LEDR2_Vol);
SysRunTime_3S=1;
return;
}
}



switch(KeyVol)
{
case 0x8000:
if(LED_ON_Sat)
{
SysDingShiEn=1;
SysRunTimeCnt=18000;


CCP1_set_duty_cycle(0);
CCP2_set_duty_cycle(0);
for(k=0;k<120;k++){ soft_delay_ms(1); WDT_clear();};
PWM_LED1(LEDR1_Vol); PWM_LED2(LEDR2_Vol);

}

break;


case 0x0080:

if(LED_ON_Sat)
{
SysDingShiEn=2;
SysRunTimeCnt=36000;

SysDingShiOne=0;
SysDingShiCnt=100;
SysDingShiSTEP=0;

}

break;


case 0x0002:
if(LED_ON_Sat)
{
if(LEDR1_Vol!=0)
{
KEYKeepAddMark_1=0;
SetRunEn(0,1,LEDR1_Vol);



i=GetChgLV(LEDR1_Vol);

n=1; if(i==0||i==1) n=4; if(i==2) n=3; if(i==3) n=2;

i = (i==0)?(ChgLV_ZX[i]):(ChgLV_ZX[i-1]);
BufSetRun(0,i,1,n);
}

if(LEDR2_Vol!=0)
{
KEYKeepAddMark_2=0;
SetRunEn(1,1,LEDR2_Vol);

i=GetChgLV(LEDR2_Vol);
n=1; if(i==0||i==1) n=4; if(i==2) n=3; if(i==3) n=2;

i = (i==0)?(ChgLV_ZX[i]):(ChgLV_ZX[i-1]);
BufSetRun(1,i,1,n);
}

}
SysDingShiEn=0;
break;

case 0x0020:
if(LED_ON_Sat)
{

if(LEDR1_Vol!=0)
{
KEYKeepSunMark_1=0;
SetRunEn(0,1,LEDR1_Vol);

i=GetChgLV(LEDR1_Vol);
n=1; if(i==0||i==1) n=4; if(i==2) n=3; if(i==3) n=2;
i = (i==5)?(ChgLV_ZX[i]):(ChgLV_ZX[i+1]);

BufSetRun(0,i,1,n);
}

if(LEDR2_Vol!=0)
{
KEYKeepSunMark_2=0;
SetRunEn(1,1,LEDR2_Vol);
i=GetChgLV(LEDR2_Vol);
n=1; if(i==0||i==1) n=4; if(i==2) n=3; if(i==3) n=2;
i = (i==5)?(ChgLV_ZX[i]):(ChgLV_ZX[i+1]);
BufSetRun(1,i,1,n);
}

}
SysDingShiEn=0;
break;


case 0x2000:
if(LED_ON_Sat)
{

if((LEDR1_Vol)&&(LEDR2_Vol))
{
k=(unsigned short int)LEDR1_Vol+(unsigned short int)LEDR2_Vol;
if(k>(unsigned short int)250) k=250;

SetRunEn(0,1,LEDR1_Vol); BufSetRun(0,k,1,3);
SetRunEn(1,1,LEDR2_Vol); BufSetRun(1,0,1,3);
}

if((LEDR1_Vol==0)&&(LEDR2_Vol))
{
i=LEDR2_Vol/2;

SetRunEn(0,1,LEDR1_Vol); BufSetRun(0,i,1,3);
SetRunEn(1,1,LEDR2_Vol); BufSetRun(1,i,1,3);
}
}
break;


case 0x0200:
if(LED_ON_Sat)
{
if((LEDR1_Vol)&&(LEDR2_Vol))
{
k=(unsigned short int)LEDR1_Vol+(unsigned short int)LEDR2_Vol;
if(k>(unsigned short int)250) k=250;

SetRunEn(0,1,LEDR1_Vol); BufSetRun(0,0,1,3);
SetRunEn(1,1,LEDR2_Vol); BufSetRun(1,k,1,3);
