const unsigned char ChgLV_ZX[6]={ 250, 100, 50, 16, 7, 2 };
const unsigned char ChgLV_FW[7]={254, 175, 75, 34, 13, 5, 0};

unsigned char GetChgLV(unsigned char vol)
{
unsigned char i=0;
for(i=0;i<5;i++)
{
if(vol>ChgLV_FW[1+i]) return i;
}

return 5;
}




